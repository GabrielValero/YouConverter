{
  "cli": {
    "version": ">= 0.47.0"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://you-converter-backend.vercel.app",
        "EXPO_PUBLIC_APP_VARIANT": "production"
      }
    }, 
    
    "preview":{
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://you-converter-backend.vercel.app",
        "EXPO_PUBLIC_APP_VARIANT": "production"
      }
    },

    "development": {
      "extends": "preview",
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development"
      }
    }

    
  },
  "submit": {
    "production": {}
  }
}
